
(:domain Human top_level_domain

    //(:include "source:Domains/movement.domain")

    (:constants
        (italian "italian")
    )

    (:constants Nationalities
        (spanish "spanish")
    )

    (:axiom (find_food ?inp_food_type ?out_food)
		(and    (food ?food_type ?food)
		)
	)

    (:axiom (is_spoiled ?inp_food)
		(and    (or (has_mold ?food)
                    (is_dry ?food)
                )
		)
	)

    (:method (entry_point) top_level_method
        (branch_eat
            (and    (#find_food @spanish ?food)
                    (not (#is_spoiled ?food))
                    (#find_food @italian ?food2)
                    (not (#is_spoiled ?food2))
            )
            (   (eat ?food)
                (eat ?food2)
            )
        )
    )

    (:method (eat ?inp_food)
        (branch_eat
            (
            )
            (   (!eat ?food)
            )
        )
    )
)
